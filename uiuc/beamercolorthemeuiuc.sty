\mode<presentation>

\RequirePackage{xcolor}

\definecolor{UIblue}{RGB}{19,41,75}
\definecolor{UIblueSub}{RGB}{5,86,165}
\definecolor{UIblueBold}{RGB}{32,64,151}
\definecolor{UIneutralLight}{RGB}{232,233,234}
\definecolor{UIneutralMedium}{RGB}{166,168,171}
\definecolor{UIneutralDark}{RGB}{94,102,105}
\definecolor{UrbanaOrange}{RGB}{232,74,39}

\definecolor{UIteal}{RGB}{13,96,94}
\definecolor{UIgrayBlue}{RGB}{111,175,199}
\definecolor{UIcitron}{RGB}{191,212,109}
\definecolor{UIdarkYellow}{RGB}{255,209,37}
\definecolor{UIsalmon}{RGB}{238,94,94}
