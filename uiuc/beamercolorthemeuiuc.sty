% % use those colors
\setbeamercolor{normal text}{fg=black,bg=UrbanaOrange!0}
\setbeamercolor{structure}{fg=UIblue}

\setbeamercolor{background canvas}{parent=normal text}
\setbeamercolor{background}{parent=background canvas}

\setbeamercolor{palette primary}{use=structure,fg=structure.fg}
\setbeamercolor{palette secondary}{use=structure,fg=UIblueBold}
\setbeamercolor{palette tertiary}{use=structure,fg=UIblueSub}
\setbeamercolor{palette quaternary}{fg=UIneutralLight}
\setbeamercolor{palette uiuc}{fg=UrbanaOrange,bg=UIblue}
\setbeamercolor{palette uiuc_alt}{fg=UIblue,bg=UrbanaOrange}
\setbeamercolor{palette titlepage}{fg=white}

\setbeamercolor{block body}{parent=normal text,fg=white,bg=UIblueSub}

\mode<all>